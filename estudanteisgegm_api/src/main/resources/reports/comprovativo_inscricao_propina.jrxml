	<queryString>
		<![CDATA[SELECT
     contacorrente.`debito` AS contacorrente_debito,
     contacorrente.`referencia` AS contacorrente_referencia,
     contacorrente.`tipo_emolumento` AS contacorrente_tipoEmolumento,
     contacorrente.`data_limite_pagamento` AS contacorrente_dataLimitePagamento,
     contacorrente.`estudante_id` AS contacorrente_estudante_id,
     contacorrente.`ano_lectivo`  AS anoLectivo,
     contacorrente.semestre AS semestre ,
     contacorrente.taxa_imposto AS imposto,
     contacorrente.total_debito As total
FROM
     contacorrente
WHERE
  contacorrente.`estudante_id` = $P{estId}
  AND ano_lectivo= $P{anoLec} AND situacao not in ('CANCELADO','OK','NEGOCIADA')]]>
