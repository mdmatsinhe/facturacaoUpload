	<queryString>
		<![CDATA[SELECT
     inscricao.`estudante_id` AS inscricao_estudante_id,
     estudante.`apelido` AS estudante_apelido,
     estudante.`nome` AS estudante_nome,
     estudante.`numero` AS estudante_numero,
      estudante.`email` AS estudante_email,
     inscricao.`ano_lectivo` AS inscricao_anoLectivo,
     inscricao.`semestre` AS inscricao_semestre,
     planoestudos.`cargaHoraria` AS planoestudos_cargaHoraria,
     planoestudos.`semestre_id` AS planoestudos_semestre_id,
     planoestudos.`anocurso_id` AS planoestudos_anocurso_id,
     disciplina.`nome` AS disciplina_nome,
     curso.`nome` AS curso_nome,
     turma.`nome` AS turma_nome,
     case when fds=true then 'SIM' else 'NÃO' end as fds 
FROM
     `estudante` estudante INNER JOIN `inscricao` inscricao ON estudante.`id` = inscricao.`estudante_id`
     INNER JOIN `planoestudos` planoestudos ON inscricao.`disciplina_semestre_id` = planoestudos.`id`
     INNER JOIN `sessaoturma` sessaoturma ON inscricao.`turma_id` = sessaoturma.`id`
       INNER JOIN `turma` turma ON turma.`id` = sessaoturma.`turma_id`
     INNER JOIN `curriculo` curriculo ON sessaoturma.`curriculo_id` = curriculo.`id`
     INNER JOIN `curso` curso ON estudante.`curso_id` = curso.`id`
     INNER JOIN `disciplina` disciplina ON planoestudos.`disciplina_id` = disciplina.`id`
WHERE
     inscricao.`estudante_id` = $P{PAR_ESTID}
 AND inscricao.`ano_lectivo` = $P{PAR_ANOL}
 AND inscricao.`semestre` = $P{PAR_SEM}]]>
	</queryString>
