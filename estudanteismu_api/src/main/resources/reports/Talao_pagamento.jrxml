	<parameter name="PAR_EMOLUMENTOS" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT curso.nome AS curso_nome,
	estudante.apelido AS estudante_apelido,
	estudante.nome AS estudante_nome,
	DENSE_RANK() OVER (ORDER BY contacorrente.`tipo_emolumento`) AS numero_ordem,
	estudante.numero AS estudante_numero,
	contacorrente.`data_limite_pagamento` AS contacorrente_dataLimitePagamento,
	contacorrente.referencia AS contacorrente_referencia,
	contacorrente.`tipo_emolumento` AS contacorrente_tipoEmolumento,
	contacorrente.total_debito,
	turma.nome as turma
  
FROM estudante estudante
	INNER JOIN contacorrente contacorrente ON 
	 estudante.id = contacorrente.estudante_id 
	INNER JOIN curso curso ON 
	 estudante.curso_id = curso.id 
	 LEFT OUTER JOIN `sessaoturma` sessaoturma ON contacorrente.`turma_id` = sessaoturma.`id`
     LEFT OUTER JOIN `turma` turma ON sessaoturma.`turma_id` = turma.`id`
where 
contacorrente.id IN ($P!{PAR_EMOLUMENTOS})
ORDER BY contacorrente.id]]>
	</queryString>
