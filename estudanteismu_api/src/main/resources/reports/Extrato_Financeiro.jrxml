	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="woodrose"/>
	<parameter name="PAR_ESTUDANTE" class="java.lang.Long"/>
	<parameter name="PAR_ANOLECTIVO" class="java.lang.Integer"/>
	<parameter name="PAR_LOGO" class="java.lang.String"/>
	<parameter name="PAR_ENTIDADE" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT  
    es.nome,
    es.apelido,
    es.responsavel_financeiro,
    es.telefone_fixo,
    es.email_responsavel_financeiro,
    fac.nome as faculdade,
    cc.ano_lectivo,
    REGEXP_REPLACE(cc.tipo_emolumento, ' *(ano lectivo 2025|do ano 2025)', '') as tipo_emolumento,
    cc.total_debito,
    cc.data_limite_pagamento,
    cc.referencia,
    CAST(COALESCE(DATE_FORMAT(cc.data_pagamento, '%d/%m/%Y'), '') AS CHAR) AS data_pagamento,
    CASE 
    WHEN cc.pago = 1 
         AND cc.valor_depositado > cc.total_debito 
         AND EXISTS (
             SELECT 1 
             FROM contacorrente cc2 
             WHERE cc2.recibo = cc.recibo 
             AND cc2.valor_depositado = cc.valor_depositado 
             AND cc2.id <> cc.id
         ) 
    THEN cc.total_debito 
    ELSE cc.valor_depositado 
END AS valor_depositado,
    (cc.total_debito - cc.valor_depositado) AS saldo,
    CASE 
        WHEN cc.recibo = 0 THEN ''
        ELSE CAST(cc.recibo AS CHAR) 
    END AS recibo,
   CASE 
        WHEN fac.id = 1 THEN 'Av. De Namaacha Km15, Chinonanquila, Matola City, Boane'
        WHEN fac.id = 2 THEN 'Liberdade: Av Das Industrias, Nº 3787, B. Liberdade - Matola'
		WHEN fac.id = 3 THEN 'Rua Joao de Barros nº 367, Bairro da Sommerschild - Maputo'
		WHEN fac.id = 4 THEN 'Rua das FLPM Muahivire, Cidade da Nampula, Nampula, Mozambique'
        ELSE 'Rua sem saída, Bairro Cimento_ Cidade de Pemba _Cabo Delgado' 
    END AS endereco,
        CASE 
        WHEN fac.id = 1 THEN 'Contacto: 87 039 4582'
        WHEN fac.id = 2 THEN 'Contacto: 87 116 9663 '
		WHEN fac.id = 3 THEN 'Contacto: 87 816 6160'
		WHEN fac.id = 4 THEN 'Contacto: 87 282 4915'
        ELSE 'Contacto: 87 897 2700' 
    END AS contacto_escola,
            CASE 
        WHEN fac.id = 1 THEN 'E-mail: treasury.matola@woodroseschool.co.mz'
        WHEN fac.id = 2 THEN 'E-mail: admin.liberdade@woodroseschool.co.mz '
		WHEN fac.id = 3 THEN 'E-mail: admin.maputo@woodroseschool.co.mz'
		WHEN fac.id = 4 THEN 'E-mail: officeexecutive1.nampula@woodroseschool.co.mz'
        ELSE 'E-mail: officemanager.pemba@woodroseschool.co.mz' 
    END AS email_escola,
                CASE 
        WHEN fac.id in (1, 4, 5) THEN 'NUIT: 400292256'
		WHEN fac.id = 2 THEN 'NUIT: 400961506'
        ELSE 'NUIT: 400240167' 
    END AS nuit_escola,
    cu.nome as classe,

    (SELECT SUM(total_debito) 
     FROM contacorrente 
     WHERE estudante_id = cc.estudante_id 
       AND ano_lectivo = cc.ano_lectivo 
       AND emolumento_id != 55) AS prop_anual,

    (SELECT SUM(total_debito) 
     FROM contacorrente 
     WHERE estudante_id = cc.estudante_id 
       AND ano_lectivo = cc.ano_lectivo 
       AND emolumento_id = 55) AS multa_anual,
       
    (SELECT SUM(total_debito) 
     FROM contacorrente 
     WHERE estudante_id = cc.estudante_id 
       AND ano_lectivo = cc.ano_lectivo
       AND data_limite_pagamento < CURDATE()) AS factura_cumul_ate,

    (SELECT SUM(total_debito) 
     FROM contacorrente 
     WHERE estudante_id = cc.estudante_id 
       AND ano_lectivo = cc.ano_lectivo) AS total_anual,

    SUM(cc.valor_depositado) AS pago_ate_data,


SUM(case when total_debito - COALESCE(valor_depositado, 0)>=0 then total_debito - COALESCE(valor_depositado, 0) else 0 end) 
OVER (PARTITION BY cc.estudante_id 
      ORDER BY 
          CASE WHEN cc.data_pagamento IS NULL THEN 1 ELSE 0 END, 
          cc.data_pagamento ASC, 
          cc.data_limite_pagamento ASC, 
          cc.id ASC 
      ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW) AS saldo_cumulativo,
    
          SUM(
        CASE 
            WHEN cc.data_limite_pagamento < CURDATE() AND pago=0
            THEN (cc.total_debito - COALESCE(cc.valor_depositado, 0)) 
            ELSE 0 
        END
    ) OVER (PARTITION BY cc.estudante_id ORDER BY cc.data_limite_pagamento 
            ROWS BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW) AS saldo_por_regularizar

FROM contacorrente cc 
INNER JOIN estudante es ON cc.estudante_id = es.id
INNER JOIN sessaoturma ss ON cc.turma_id = ss.id
INNER JOIN curso cu ON ss.curso_id = cu.id
inner join faculdade fac on cu.faculdade_id=fac.id

WHERE
    cc.estudante_id = $P{PAR_ESTUDANTE}
    AND cc.ano_lectivo = $P{PAR_ANOLECTIVO}

GROUP BY 
    es.nome, es.apelido, es.responsavel_financeiro, es.telefone_fixo, es.email_responsavel_financeiro,
    fac.nome, fac.id, 
    cc.ano_lectivo, cc.tipo_emolumento, cc.total_debito, cc.data_limite_pagamento, 
    cc.referencia, cc.data_pagamento, cc.pago, cc.valor_depositado, cc.recibo, cu.nome, cc.id
ORDER BY cc.pago DESC, cc.data_limite_pagamento, cc.data_pagamento;]]>
	</queryString>
